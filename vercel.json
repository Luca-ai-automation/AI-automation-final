{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "server/chat.js": { "runtime": "nodejs20.x" },
    "server/lead.js": { "runtime": "nodejs20.x" }
  },
  "rewrites": [
    { "source": "/chat", "destination": "/server/chat.js" },
    { "source": "/lead", "destination": "/server/lead.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
